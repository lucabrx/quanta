		"sveltekit-superforms": "^0.8.7",
		"tailwindcss": "^3.1.5",
		"tslib": "^2.4.1",
		"tsx": "^3.12.7",
		"typescript": "^5.0.0",
		"vite": "^4.3.0",
		"vitest": "^0.25.3",
		"zod": "^3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.9.4",
